{
  "manifest_version": 3,
  "name": "Geeks for Geeks Pop-Up Blocker",
  "version": "1.0",
  "description": "This extension removes the sign-in popup on Geeks for Geeks, allowing you to access the site's content without logging in.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://www.geeksforgeeks.org/*"
      ]
    }
  ]
}